{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Oidc": {
    "Authority": "http://host.docker.internal:5002/",
    "ClientId": "frontend2",
    "DefaultScopes": [
      "openid",
      "profile",
      "catalog.fullaccess",
      "catalog.readaccess",
      "catalog.writeaccess", 
      "inventory.fullaccess",
      "trading.fullaccess",
      "IdentityServerApi",
      "roles"
    ],
    "PostLogoutRedirectUri": "http://host.docker.internal:5008/",
    "ResponseType": "code",
    "RedirectUrl":  "http://host.docker.internal:5008/"
    
  },
  "AllowedOrigins": ["http://host.docker.internal:5002","http://host.docker.internal:5000"]

}
